<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practice.lcn12.myapplication" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.practice.lcn12.myapplication.MainActivity" />
        </activity>
        <activity
            android:name=".FrontPage"
            android:label="@string/title_activity_front_page" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CToMIPSActivity"
            android:label="@string/title_activity_cto_mips" >
        </activity>
        <activity
            android:name=".MIPSRefActivity"
            android:label="@string/title_activity_mips_ref" >
        </activity>
        <activity
            android:name=".ParallelizedAmdahlsActivity"
            android:label="@string/title_activity_parallelized_amdahls" >
        </activity>
    </application>

</manifest>
